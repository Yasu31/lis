<script type="text/javascript">
function shaker(i){
increment = Math.random()*100+50;
if (i%4 ==0)
moveBy(-increment, 0);
else if (i%4 ==1)
moveBy(0, increment);
else if (i%4 ==2)
moveBy(increment, 0);
else
moveBy(0, -increment);
if (i<100)
setTimeout(function() {shaker(++i)}, 100);
}
resizeTo(150, 330);
shaker(0);
//alert("hi");
</script>
